{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 675,
    "height": 400,
    "title": "Each States Highest Educations % of Populations",
    "data": {
      "url": "https://raw.githubusercontent.com/deanmasc/FIT3179-Assignment2/refs/heads/main/data/highest_edu.csv",
      "format": {"type": "csv"}
    },
    "mark": "circle",
    "encoding": {
      "x": {
        "field": "State",
        "type": "nominal",
        "axis": {
          "title": "State",
          "labelAngle": 0  
        }
      },
      "y": {
        "field": "Highest Education",
        "type": "nominal",
        "axis": {"title": "Highest Education"},
        "sort": ["Postgraduate", "Graduate Diploma", "Bachelor", "Certificate III/IV", "Year 12 or Equivilent"] 
      },
      "size": {
        "field": "Norm Amount",
        "type": "quantitative",
        "scale": {"range": [0, 5000]}, 
        "legend": {"title": "% of State Population"}
      },
      "color": {
        "field": "Highest Education",
        "type": "nominal",
        "scale": {
          "domain": ["Postgraduate", "Graduate Diploma", "Bachelor", "Certificate III/IV", "Year 12 or Equivilent"],
          "range": ["#800000", "#b30000", "#e60000", "#ff3333", "#ff6666"]  
        },
        "legend": {"title": "Highest Education"}
      },
      "tooltip": [
        {"field": "State", "title": "State"},
        {"field": "Highest Education", "title": "Education"},
        {"field": "% of State Population", "title": "Norm Amount", "type": "quantitative"}
      ]
    }
  }
  