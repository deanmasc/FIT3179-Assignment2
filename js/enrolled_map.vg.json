{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": "% of Australians With at Least High School Certification Education Level by State (2023)",
    "data": {
      "url": "https://raw.githubusercontent.com/deanmasc/FIT3179-Assignment2/refs/heads/main/js/states.topojson",
      "format": {
        "type": "topojson",
        "feature": "states"
      }
    },
    "transform": [
      {
        "lookup": "properties.STATE_NAME",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/deanmasc/FIT3179-Assignment2/refs/heads/main/data/Enrolled_in_study_by%20sex_and_state.csv",
            "format": {"type": "csv"}
          },
          "key": "State",
          "fields": ["Male_enrolled", "Female_enrolled", "Other_enrolled"]
        }
      }
    ],
    "projection": {
      "type": "mercator"
    },
    "mark": {
      "type": "geoshape",
      "stroke": "white",
      "strokeWidth": 1
    },
    "params": [ {
      "name": "gender_selection",
      "value": "Male_enrolled",  
      "bind": {
        "input": "select",
        "options": ["Male_enrolled", "Female_enrolled", "Other_enrolled"],
        "labels": ["Male", "Female", "Other"],
        "name": "Select Gender: "
      }
    }],

    "encoding": {
      "color": {
        "field": {"expr": "gender_selection"},
        "type": "quantitative",
        "title": "% of People"
      },
      "tooltip": [
        {"field": "properties.STATE_NAME", "type": "nominal", "title": "State"},
        {"field": {"expr": "gender_selection"}, "title": "% of People", "type": "quantitative"}
      ]
    }
  }